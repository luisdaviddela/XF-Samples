<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Labels"
             x:Class="LDXF.Ej2">
    <ScrollView>
        <StackLayout>
            <Entry MaxLength="2" />
            <Entry MaxLength="5" InputTransparent="True"/>
            <Entry MaxLength="5" HorizontalTextAlignment="Center"/>
            <Entry Placeholder="Numeric" Keyboard="Numeric" />
            <Entry Placeholder="Url" Keyboard="Url" />
            <Entry Placeholder="Telephone" Keyboard="Telephone" />
        </StackLayout>
    </ScrollView>
</ContentPage>